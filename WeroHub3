local function EIFLMAH_fake_script() -- Diplay_2.LocalScript 
	local script = Instance.new('LocalScript', Diplay_2)

	-- Referencias
	local button = script.Parent -- El bot√≥n (TextButton)
	local player = game.Players.LocalPlayer
	local savedPos = nil
	
	-- Crear un mensaje en pantalla
	local ScreenGui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
	local messageLabel = Instance.new("TextLabel", ScreenGui)
	messageLabel.Size = UDim2.new(0.3, 0, 0.1, 0)
	messageLabel.Position = UDim2.new(0.35, 0, 0.8, 0)
	messageLabel.BackgroundTransparency = 0.3
	messageLabel.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	messageLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	messageLabel.TextScaled = true
	messageLabel.Text = ""
	messageLabel.Visible = false
	
	-- Funci√≥n al presionar el bot√≥n
	button.MouseButton1Click:Connect(function()
		local char = player.Character or player.CharacterAdded:Wait()
		local root = char:WaitForChild("HumanoidRootPart")
		savedPos = root.Position -- Guarda la posici√≥n actual
		print("Posici√≥n guardada:", savedPos)
	
		-- Mostrar mensaje
		messageLabel.Text = "‚úÖ Posici√≥n guardada"
		messageLabel.Visible = true
	
		-- Ocultar el mensaje despu√©s de 2 segundos
		task.delay(2, function()
			messageLabel.Visible = false
		end)
	end)
	
end
coroutine.wrap(EIFLMAH_fake_script)()
local function MDNEWM_fake_script() -- part_3.LocalScript 
	local script = Instance.new('LocalScript', part_3)

	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	
	local savedPos = nil
	local goingToPos = false
	local humanoid = nil
	local cancelButton = nil
	local jumpBoost = 80 -- fuerza constante del salto
	
	-- Botones existentes en tu GUI
	local saveButton = script.Parent.Parent.Diplay -- Guardar posici√≥n
	local gotoButton = script.Parent.Parent.part   -- Ir a la posici√≥n
	
	-- Guardar posici√≥n actual
	saveButton.MouseButton1Click:Connect(function()
		local char = player.Character or player.CharacterAdded:Wait()
		local root = char:WaitForChild("HumanoidRootPart")
		savedPos = root.Position
		print("‚úÖ Posici√≥n guardada:", savedPos)
	end)
	
	-- Crear bot√≥n Cancel din√°micamente
	local function createCancelButton()
		if cancelButton then cancelButton:Destroy() end
	
		cancelButton = Instance.new("TextButton")
		cancelButton.Name = "Cancel"
		cancelButton.Size = UDim2.new(0, 120, 0, 40)
		cancelButton.Position = UDim2.new(0, 20, 0, 200)
		cancelButton.BackgroundColor3 = Color3.fromRGB(150, 0, 0)
		cancelButton.TextColor3 = Color3.fromRGB(255, 255, 255)
		cancelButton.Text = "Cancelar"
		cancelButton.Parent = script.Parent
	
		local corner = Instance.new("UICorner")
		corner.CornerRadius = UDim.new(0, 8)
		corner.Parent = cancelButton
	
		-- Acci√≥n al presionar Cancel
		cancelButton.MouseButton1Click:Connect(function()
			if goingToPos then
				goingToPos = false
				if humanoid then
					humanoid:Move(Vector3.new(0,0,0))
				end
				print("üõë Movimiento cancelado")
			end
			cancelButton:Destroy()
			cancelButton = nil
		end)
	end
	
	-- Ir a la posici√≥n guardada
	gotoButton.MouseButton1Click:Connect(function()
		if not savedPos then return end
		local char = player.Character or player.CharacterAdded:Wait()
		humanoid = char:WaitForChild("Humanoid")
		local rootPart = char:WaitForChild("HumanoidRootPart")
	
		goingToPos = true
		createCancelButton()
	
		-- Primer salto inmediato con mismo boost que los dem√°s
		rootPart.Velocity = Vector3.new(rootPart.Velocity.X, jumpBoost, rootPart.Velocity.Z)
	
		-- Detectar cuando toca el suelo y dar boost con Velocity
		local connection
		connection = humanoid.StateChanged:Connect(function(_, newState)
			if goingToPos and newState == Enum.HumanoidStateType.Landed then
				rootPart.Velocity = Vector3.new(rootPart.Velocity.X, jumpBoost, rootPart.Velocity.Z)
			end
		end)
	
		-- Bucle de movimiento hasta llegar
		task.spawn(function()
			while goingToPos and humanoid and (rootPart.Position - savedPos).Magnitude > 3 do
				humanoid:MoveTo(savedPos)
				task.wait(0.2)
			end
	
			-- Detener saltos al llegar
			goingToPos = false
			if connection then connection:Disconnect() end
			if cancelButton then
				cancelButton:Destroy()
				cancelButton = nil
			end
		end)
	end)
	
end
coroutine.wrap(MDNEWM_fake_script)()
