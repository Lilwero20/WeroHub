coroutine.wrap(LQYQNMF_fake_script)()
local function WASEMQ_fake_script() -- Diplay.LocalScript 
	local script = Instance.new('LocalScript', Diplay)

	local button = script.Parent
	local boost = script.Parent.Parent.Parent.Boost
	
	-- Colores del bot√≥n
	local colorOn = Color3.fromRGB(0, 255, 0)    -- Verde
	local colorOff = Color3.fromRGB(255, 100, 100) -- Gris
	
	-- Inicializar estado
	boost.Visible = false
	button.BackgroundColor3 = colorOff
	button.Text = "Display Jump Boost"
	
	-- Conectar la acci√≥n al bot√≥n
	button.MouseButton1Click:Connect(function()
		boost.Visible = not boost.Visible -- Alterna la visibilidad
	
		if boost.Visible then
			button.BackgroundColor3 = colorOn
			button.Text = "Displaying Jump Boost"
		else
			button.BackgroundColor3 = colorOff
			button.Text = "Display Jump Boost"
		end
	end)
	
end
