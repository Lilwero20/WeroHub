-- Scripts:

local function NXDEYR_fake_script() -- Boost.LocalScript 
	local script = Instance.new('LocalScript', Boost)

	-- LocalScript dentro del ScreenGui o el ScrollingFrame
	local frame = script.Parent
	local dragging = false
	local dragStart
	local startPos
	
	local function startDrag(input)
		dragging = true
		dragStart = input.Position
		startPos = frame.Position
	
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
	
	local function drag(input)
		if dragging then
			local delta = input.Position - dragStart
			frame.Position = UDim2.new(
				startPos.X.Scale,
				startPos.X.Offset + delta.X,
				startPos.Y.Scale,
				startPos.Y.Offset + delta.Y
			)
		end
	end
	
	-- Detectar inicio de arrastre (mouse o touch)
	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			startDrag(input)
		end
	end)
	
	-- Detectar movimiento (mouse o touch)
	frame.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			drag(input)
		end
	end)
	
end
coroutine.wrap(NXDEYR_fake_script)()
local function PTMKWWZ_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local Players = game:GetService("Players")
	local UserInputService = game:GetService("UserInputService")
	local player = Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local rootPart = character:WaitForChild("HumanoidRootPart")
	
	-- TextButton
	local myButton = script.Parent.Parent.TextButton
	
	-- Variable para controlar si el Jump Boost est√° activo
	local jumpBoostEnabled = false
	
	-- Colores de fondo
	local colorOn = Color3.fromRGB(0, 255, 0)   -- Verde cuando est√° activado
	local colorOff = Color3.fromRGB(128, 128, 128) -- Gris cuando est√° desactivado
	
	-- Inicializar el bot√≥n
	myButton.BackgroundColor3 = colorOff
	myButton.Text = "Jump Boost: Off"
	
	-- Conectar la acci√≥n al bot√≥n
	myButton.MouseButton1Click:Connect(function()
		jumpBoostEnabled = not jumpBoostEnabled -- Alterna entre activado y desactivado
		if jumpBoostEnabled then
			myButton.BackgroundColor3 = colorOn
			myButton.Text = "Jump Boost: On"
		else
			myButton.BackgroundColor3 = colorOff
			myButton.Text = "Jump Boost: Off"
		end
	end)
	
	-- Detectar salto
	UserInputService.JumpRequest:Connect(function()
		if jumpBoostEnabled and rootPart then
			rootPart.Velocity = Vector3.new(rootPart.Velocity.X, 75, rootPart.Velocity.Z)
		end
	end)
	
end
coroutine.wrap(PTMKWWZ_fake_script)()
