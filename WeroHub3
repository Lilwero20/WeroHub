local function QUAX_fake_script() -- mn.LocalScript 
	local script = Instance.new('LocalScript', mn)

	local ds = script.Parent.Parent.Display
	local part = script.Parent.Parent.part
	local diplay = script.Parent.Parent.Diplay
	local spe = script.Parent.Parent.spe
	local speb = script.Parent.Parent["spe brain"]
	
	script.Parent.MouseButton1Click:Connect(function()
		ds.Visible = true
		part.Visible = true
		diplay.Visible = true
		spe.Visible = false
		speb.Visible = false
	end)
end
